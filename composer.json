{
    "name": "bradleyhodges/api-manager",
    "description": "A powerful, robust, secure, and compliant API manager for PHP-based APIs",
    "type": "library",
    "version": "4.0.1",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "APIManager\\": "src/"
        }
    },
    "keywords": ["api", "manager", "cors", "rate limiter", "php"],
    "homepage": "https://github.com/dfes-ses/api-manager",
    "require": {
        "php": ">=8.2",
        "sergeytsalkov/meekrodb": "^3.0",
        "guzzlehttp/guzzle": "^7.9",
        "monolog/monolog": "^3.7",
        "giggsey/libphonenumber-for-php": "^8.13",
        "robinvdvleuten/ulid": "^5.0",
        "vlucas/phpdotenv": "^5.6",
        "nesbot/carbon": "^3.8",
        "symfony/validator": "^7.1",
        "league/flysystem": "3.0",
        "lcobucci/jwt": "^4.0",
        "symfony/rate-limiter": "^7.1",
        "symfony/security-csrf": "^7.1",
        "phpseclib/phpseclib": "^3.0",
        "sentry/sentry": "^4.9",
        "symfony/cache": "^7.1",
        "spatie/guzzle-rate-limiter-middleware": "^2.0",
        "react/socket": "^1.16",
        "psr/simple-cache": "^3.0",
        "react/event-loop": "^1.5",
        "react/dns": "^1.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "authors": [
        {
            "name": "Bradley Hodges",
            "email": "bradley.hodges@outlook.com"
        }
    ],
    "minimum-stability": "stable"
}
